    "# Dataset №1 \n",
    "# Forest Fires Data Set\n",
    "# https://archive.ics.uci.edu/ml/datasets/Forest+Fires\n",
    "path_dataset_1='forestfires1.csv'\n",
    "\n",
    "# Dataset №2 \n",
